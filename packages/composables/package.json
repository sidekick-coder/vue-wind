{
  "name": "@vue-wind/composables",
  "version": "0.0.1",
  "description": "Composables & macros from vue-wind",
  "keywords": [
    "vue",
    "tailwind"
  ],
  "author": "Henrique",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "test": "vitest",
    "build": "tsup"
  },
  "dependencies": {
    "vue": "^3.2.47"
  }
}