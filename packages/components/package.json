{
  "name": "@vue-wind/components",
  "version": "0.0.1",
  "author": "Henrique",
  "license": "MIT",
  "description": "Vue components using tailwind",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vue",
    "tailwind",
    "components"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.es.js",
      "require": "./dist/*.umd.js",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "test": "vitest",
    "build": "tsx build.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@vue-wind/composables": "^0.0.1"
  }
}